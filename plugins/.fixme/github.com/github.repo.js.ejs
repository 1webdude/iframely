function init($container, cb){

    var $repohead = $('<div />');
    var $repocontent = $('<div />');

    var repoId = '<%- repoId %>';

    // TODO: remove folders structure widget.

    $container.append($repohead)
    $container.append($repocontent);
    $repohead.githubrepo(repoId, function(){
        $repohead.find('a').attr('target','_blank');
        var repoIdParts = repoId.split('/');
        if (repoIdParts.length > 1 && repoIdParts[0] && repoIdParts[1]){
            $repocontent.repo({user: repoIdParts[0], name: repoIdParts[1]});
        }
        cb();
    });
}